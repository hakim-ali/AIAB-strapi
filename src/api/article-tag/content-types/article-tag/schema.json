{
  "kind": "collectionType",
  "collectionName": "article_tags",
  "info": {
    "singularName": "article-tag",
    "pluralName": "article-tags",
    "displayName": "ArticleTag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "article_tags"
    },
    "color": {
      "type": "enumeration",
      "required": false,
      "default": "primary",
      "enum": [
        "primary",
        "success",
        "error",
        "warning",
        "info",
        "purple",
        "success-light"
      ]
    },
    "secondary_articles_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "secondary_article_tags"
    }
  }
}
